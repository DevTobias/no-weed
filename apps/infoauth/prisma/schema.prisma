generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["filterJson"]

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(uuid())
  username String   @unique
  email    String   @unique
  password String
  payload  String?
  tokens   String[]
}
