{
  "name": "info-auth",
  "version": "1.0.0",
  "license": "MIT",
  "author": "infotition",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/DevTobias/info-planner.git",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsi watch --format cjs",
    "dev:start": "cross-env NODE_ENV=development npx nodemon ./lib/index.js",
    "test": "cross-env NODE_ENV=test tsi test",
    "lint": "tsi lint",
    "build": "cross-env NODE_ENV=production tsi build --format cjs",
    "clean": "tsi clean",
    "bundle": "tsi publish --dry",
    "prisma:push": "npx prisma db push",
    "prisma:dev": "npx prisma studio",
    "prisma:seed": "npx prisma db seed",
    "docker": "docker build . -t infotition/infoauth"
  },
  "dependencies": {
    "@infotition/express-error-handler": "^1.0.0",
    "@prisma/client": "^3.13.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "http-status": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pm2": "^5.2.0",
    "prisma": "^3.13.0",
    "redis": "^4.1.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.7.2",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@infotition/eslint-config": "^2.2.0",
    "@infotition/prettier-config": "^1.0.0",
    "@infotition/tsi": "^2.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.25",
    "@types/serve-favicon": "^2.5.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "husky": "^7.0.4",
    "typescript": "^4.6.3"
  },
  "prisma": {
    "seed": "npx ts-node prisma/seed.ts"
  },
  "prettier": "@infotition/prettier-config",
  "eslintConfig": {
    "extends": [
      "@infotition/eslint-config"
    ],
    "ignorePatterns": [
      "*.js",
      "lib/**"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
